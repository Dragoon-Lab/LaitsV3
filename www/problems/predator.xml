            <Plan>proportional to accumulator and input</Plan>
            <Inputs>
                <Name>Water Fowl</Name>
                <Name>Rabbit Death Rate</Name>
                <Name>Bobcat Population</Name>
            </Inputs>
            <Outputs/>
            <Equation>Water Fowl * Rabbit Death Rate * Bobcat Population</Equation>
            <InitialValue>0.0</InitialValue>
        </Node>
        <Node type="flow" name="Bobcat Births Per Day" extra="no">
