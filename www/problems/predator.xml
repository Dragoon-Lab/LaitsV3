            <Inputs>
                <Name>Water Fowl</Name>
                <Name>Rabbit Birth Rate</Name>
            </Inputs>
            <Outputs/>
            <Equation>Water Fowl * Rabbit Birth Rate</Equation>
            <InitialValue>0.0</InitialValue>
        </Node>
        <Node type="flow" name="Rabbit Deaths Per Day" extra="no">
