            <Plan>proportional to accumulator and input</Plan>
            <Inputs>
                <Name>Rabbit Births Per Day</Name>
                <Name>Rabbit Deaths Per Day</Name>
            </Inputs>
            <Outputs/>
            <Equation>Rabbit Births Per Day -  Rabbit Deaths Per Day</Equation>
            <InitialValue>10.0</InitialValue>
        </Node>
        <Node type="stock" name="Bobcat Population" extra="no">
