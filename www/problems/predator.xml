            <Plan>proportional to accumulator and input</Plan>
            <Inputs>
                <Name>Water Fowl</Name>
                <Name>Bobcat Birth Rate</Name>
                <Name>Bobcat Population</Name>
            </Inputs>
            <Outputs/>
            <Equation>Water Fowl * Bobcat Birth Rate * Bobcat Population</Equation>
            <InitialValue>0.0</InitialValue>
        </Node>
        <Node type="flow" name="Bobcat Deaths Per Day" extra="no">
            <CorrectDescription>The number of bobcats removed from the population each day</CorrectDescription>
            <Plan>proportional to accumulator and input</Plan>
            <Inputs>
                <Name>Bobcat Population</Name>
                <Name>Bobcat Death Rate</Name>
            </Inputs>
            <Outputs/>
            <Equation>Bobcat Population *  Bobcat Death Rate</Equation>
            <InitialValue>0.0</InitialValue>
        </Node>
        <Node type="stock" name="Water Fowl" extra="no">
